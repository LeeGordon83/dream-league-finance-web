<div class="tab-pane fade show active" id="fivers-meeting" role="tabpanel" aria-labelledby="fivers-meeting-tab">
    <br/>
    <h4 style="text-align:center">Fivers By Meeting</h4>
    <br/>
      <table class="table table-hover table-sm small">
        <tr>
            <th style="text-align:center">Month</th>
            <th style="text-align:center">Winners</th>
        </tr>
        {% for month in winnings.fiversByMonth %}
                <tr>
                    <td style="text-align:center">{{ month.monthName }}</td>
                    <td style="text-align:center">
                            {% for name in month.names %}
                                {{ name }}{% if not loop.last %}, {% endif %}
                            {% endfor %}
                    </td>
                </tr>
        {% endfor %}

    </table>
</div>
