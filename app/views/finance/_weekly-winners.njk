<div class="tab-pane fade" id="weekly-winners" role="tabpanel" aria-labelledby="weekly-winners-tab">
    <br/>
    <h4 style="text-align:center">Weekly Winners</h4>
    <br/>
    <table class="table table-hover table-sm small">
        <tr>
            <th style="text-align:center">Week</th>
            <th style="text-align:center">Winners</th>
        </tr>
        {% for week in winnings.weeklyWinners %}
            <tr>
                <td style="text-align:center">{{ week.weekId }}</td>
                <td style="text-align:center">
                    {% for name in week.managerNames %}
                        {{ name }}
                        {% if not loop.last %}, {% endif %}
                    {% endfor %}
                </td>
            </tr>
        {% endfor %}

    </table>
</div>
